#include "math-lib.h"

double ml_power(double n1, double n2)
/*
 * Level 0
 *      assume n1, n2 >= 0 and n2 is a whole number stored in a double type var.
 * Level 1
 *      assume n1, n2 >= 0 are Real numbers
 * Level 2
 *      no assumptions
*/

// Level 1
{
int i=0,num=1;
	if (n2==0)
		return 1;
	else
	{
	for(i=1;i<=n2;i++)
		{
			num=(num*n1);
		}	
	return (num);
	}


	    return 0;
}
